.logged-in .sign-up {
	left:1px;
}

.sign-up {	
	font-size: 30px;
	font-size: 1.875rem;
	left:-7px;
	
	
	
	a {
		box-sizing: border-box;
		display: block;
		position: relative;
		background-color: $white;
		//width:90%;
		padding-left: 20px !important;
		padding-right:20px !important;
		//margin:0 auto !important;
		//padding: 22px 10px 14px 8px;
		text-align: center;
		color:$orange !important;
		text-decoration: none;
		
		&:after {
			position: absolute;
			display: block;
			width:100%;
			height:16px;
			left: 0;
			bottom: -16px;
			z-index: 3000;
			content:"";
			background: url('../images/nav-bar-decor-white.png') center right 1px repeat-x;
			
		}

		&:hover {
			color:$orange;
			text-decoration: underline;
		}
		
		
	}
}

