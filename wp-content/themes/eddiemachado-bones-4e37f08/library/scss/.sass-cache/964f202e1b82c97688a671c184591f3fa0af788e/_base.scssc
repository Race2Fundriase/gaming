  input[type=password],
  textarea,
  select {
    width: 100%; 
  }
}
@media (min-width: 1140px) {
  form > div {
	top:-62px;
  }
  form > div > label,
	legend {
  	text-align: right;
  }
}

/*********************
 Tabbed Content Form
*********************/

.tabbed_content {
	display: none;	
}

.tabbed_content.active {
	display: block;
}

/**********************
 *Active Race Profile
 **********************/

.active-race {
	padding-top: 20px;
	padding-bottom: 50px;
	text-align: center;
	h2 {
		margin-bottom: 3px;
	}
	
	h3 {
		margin-top:3px;
		margin-bottom: 10px;
	}
	ol {
		text-align: left;
		list-style-type: decimal;
		list-style-position: inside;
		margin-top:2px;
		li {
			width: 100%;
			border-bottom: 2px solid $black;
			padding: 0 1em 0 1em;
		}
	}
	
	.headings, .dates {
		display: block;
		height:24px;
		position:relative;
		background-color: #fff;
		
		p:first-of-type, time:first-of-type {
			position:absolute;
			margin:0;
			left:1em;
			top:0;
		}
		
		p:last-of-type, time:last-of-type {
			position:absolute;
			margin:0;
			top:0;
			right:1em;
		}
	}
	
	.headings {
		margin-top: 0.8rem;
	}
	
	.dates {
		margin-top: 2px;
	}
	
	&:after {
		content:" ";
		display: table;
		clear: both;
	}
	
	.btn {
		position: absolute;
		right: 0;
		bottom: 0;
	}
}

.active-race.last-on-page {
		padding-bottom: 80px;
		
		.btn {
			bottom:30px;
		}
	}

/*************************
 *Active Race Page / Complete Race
 *************************/

#active-race-header, #complete-race-header {
	position:relative;
	
	img {
		position: relative;
		top:0;
		left: 0;
	}
	
	#other-search {
		position: absolute;
		width:90%;
		top:20px;
		z-index: 20;
		
		input {
			width:86%;
			
			&:focus {
				width:86%;
			}
		}
	}
	
	.info {
		position: absolute;
		width:100%;
		z-index: 20;
		bottom: 0;
	}
	
	.btn {
		position:relative;
		font-size: 28px;
		left:10px;
		bottom:-40px;
	}
	
	.headings, .dates {
		display: block;
		z-index: 20;
		bottom:15px;
		left:100%;
		margin-left:-150px;
		width:150px;
		height:20px;
		position:relative;
		background-color: #fff;
		font-size: 80%;
		
		p:first-of-type, time:first-of-type {
			position:absolute;
			margin:0;
			left:1em;
			top:0;
		}
		
		p:last-of-type, time:last-of-type {
			position:absolute;
			margin:0;
			top:0;
			right:1em;
		}
	}
	
	.headings {
		margin-top: 0.8rem;
	}
	
	.dates {
		margin-top: 2px;
	}
}

#race-data {
	
	p {
		padding: 0.1em 0.3em;
	}
	
	dd, dt {
		padding-top: 0.1em;
		padding-bottom: 0.1em;
		display: inline;
		background-color: #fff;
		color: $orange;
	}
	
	dt {
		padding-left:0.3em; 
	}
	
	dd {
		padding-right: 0.3em;
	}
	
	dd:after {
		content: "\a";
		white-space: pre;
	}
}

#race-data .characters {
	margin-top: 30px;	
}

#race-data .leaderboard {
	width:200px;
	margin-top: 30px;
	ol {
		text-align: left;
		list-style-type: decimal;
		list-style-position: inside;
		margin-top:2px;
		
		li {
			border-bottom: 2px solid $black;
			padding: 0 1em 0 1em;
		}
	}
	
	table {
		width:612px;
	}
	
	
	th {
	  font-weight: normal;
	  border-bottom: 8px solid #000;
	}
	
	td {
		border-bottom: 2px solid #000;
	}
	td, th {
	  padding: 0 0.3em; 
	  background-color: #fff;
	  color:$orange;
	  text-align: left; 
	}
}

#profile-excerpt {
	
	.profile-view {
		margin-top: 10px;
	}
	p a {
		color:$orange;
		&:hover {
			
		}
	}
}

.winner {
	font-family: "Headline One";
	font-size: 40px;
	font-size: 2.5rem;
	letter-spacing: 1px;
	text-align: center;
	text-transform: uppercase;
	background: $orange;
	padding: 0.5em;
	color:#fff;
	position:relative;
	top:-25px;
	margin-bottom:-30px;
}

/*********************
SIDEBARS & ASIDES
*********************/

.sidebar {}

	.widgettitle {}

	.widget {

		ul {

			li {

				&:first-child {}

				&:last-child {}

				a {}

				/* deep nesting */
				ul {}

			}

		}

	}

/*********************
FOOTER STYLES
*********************/

.footer {
	clear: both;
	background: url('../images/sand-bg.gif');
}

	/*
	if you checked out the link above:
	http://www.alistapart.com/articles/organizing-mobile/
	you'll want to style the footer nav
	a bit more in-depth. Remember to keep
	it simple because you'll have to
	override these styles for the desktop
	view.
	*/
	.footer-links {

		ul {

			li {}

		}

	} /* end .footer-links */


	.copyright {}

